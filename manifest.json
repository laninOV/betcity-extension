{
  "manifest_version": 3,
  "name": "Tennis Analysis Extension Pro",
  "version": "2.0",
  "description": "Продвинутый анализ матчей настольного тенниса",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
